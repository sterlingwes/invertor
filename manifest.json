{
  "name": "Invertor",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Inverts a website's colours",
  "homepage_url": "https://github.com/sterlingwes/invertor",
  "icons": {
    "16": "icons/icon_dark16.png",
    "48": "icons/icon_dark48.png",
    "128": "icons/icon_dark128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_dark32.png",
    "default_title": "invertor - invert page"
  },
  "options_ui": {
    "page": "src/options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage"
  ]
}