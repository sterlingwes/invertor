{
  "name": "Invertor",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Inverts a website's colours",
  "homepage_url": "https://github.com/sterlingwes/invertor",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "invert page"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "<all_urls>"
  ]
}